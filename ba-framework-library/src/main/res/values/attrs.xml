<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MySecurityInputArea">
        <attr name="length" format="reference|integer" />
        <attr name="inputCellWidth" format="dimension" />
        <attr name="inputCellHeight" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LabelInputView">
        <attr name="label" format="reference|string" />
        <attr name="hint" format="reference|string" />
        <attr name="inputType" format="reference|integer" />
        <attr name="innerEditClickable" format="boolean" />
        <attr name="textSize" format="dimension" />
        <attr name="labelTextSize" format="dimension" />
        <attr name="multiline" format="boolean" />
        <attr name="isLabelAnimate" format="boolean" />
    </declare-styleable>
</resources>